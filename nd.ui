<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="ttk.Frame" id="frame1">
    <property name="height">400</property>
    <property name="width">400</property>
    <layout manager="pack">
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="tk.Text" id="text1">
        <property name="height">10</property>
        <property name="width">50</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="height">150</property>
          <property name="x">0</property>
          <property name="y">203</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="btnGhi">
        <property name="command" type="command" cbtype="with_wid">btnGhi_clicked</property>
        <property name="takefocus">false</property>
        <property name="text" translatable="yes">Ghi âm</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relwidth">0.45</property>
          <property name="x">0</property>
          <property name="y">50</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="btnTai">
        <property name="command" type="command" cbtype="simple">btnTai_clicked</property>
        <property name="takefocus">false</property>
        <property name="text" translatable="yes">Tải lên</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relwidth">0.45</property>
          <property name="x">220</property>
          <property name="y">50</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="btnGhi_dung">
        <property name="command" type="command" cbtype="with_wid">btnGhi_dung_clicked</property>
        <property name="takefocus">false</property>
        <property name="text" translatable="yes">Dừng</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="x">10</property>
          <property name="y">80</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="btnGhi_nghe">
        <property name="command" type="command" cbtype="with_wid">btnGhi_nghr_clicked</property>
        <property name="takefocus">false</property>
        <property name="text" translatable="yes">Nghe</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="x">90</property>
          <property name="y">80</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="btnNhanDang">
        <property name="command" type="command" cbtype="with_wid">btnNhanDang_clicked</property>
        <property name="text" translatable="yes">Nhận dạng</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relheight">0.14</property>
          <property name="relwidth">0.84</property>
          <property name="x">30</property>
          <property name="y">135</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="btnXuat">
        <property name="command" type="command" cbtype="simple">btnXuat_clicked</property>
        <property name="text" translatable="yes">Xuất kết quả</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relheight">0.08</property>
          <property name="relwidth">0.26</property>
          <property name="x">290</property>
          <property name="y">360</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
